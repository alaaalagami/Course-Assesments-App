<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LoginBoundary">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="400.0" prefWidth="542.0" VBox.vgrow="ALWAYS">
      <children>
            <TextField fx:id="UsernameTextField" layoutX="274.0" layoutY="113.0" />
            <PasswordField fx:id="PasswordTextField" layoutX="274.0" layoutY="164.0" />
            <Text layoutX="190.0" layoutY="131.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username" />
            <Text layoutX="194.0" layoutY="182.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password" />
            <Button fx:id="LoginButton" layoutX="395.0" layoutY="309.0" mnemonicParsing="false" onAction="#LoginButtonClicked" text="login" />
            <Button fx:id="SignUpButton" layoutX="481.0" layoutY="309.0" mnemonicParsing="false" onAction="#signUpButtonClicked" text="signup" />
      </children>
    </AnchorPane>
  </children>
</VBox>
