<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CoursesBoundary">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <TableView fx:id="courseListTableView" layoutX="14.0" layoutY="14.0" prefHeight="183.0" prefWidth="615.0">
              <columns>
                <TableColumn fx:id="courseTableColumn" prefWidth="75.0" text="Courses" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <Button fx:id="addCourseButton" layoutX="415.0" layoutY="226.0" mnemonicParsing="false" onAction="#addCourseButtonClicked" text="Add" />
            <Button fx:id="remCourseButton" layoutX="481.0" layoutY="226.0" mnemonicParsing="false" onAction="#remCourseButtonClicked" text="Remove" />
            <Button fx:id="nextButton" layoutX="504.0" layoutY="326.0" mnemonicParsing="false" onAction="#nextButtonClicked" text="Next" />
      </children>
    </AnchorPane>
  </children>
</VBox>
